pipeline {
   agent {label 'default'}
    stages {
        stage ('vcs') {
            steps {
                git url:'https://github.com/gautamshrinivas/pipelines.git',
                branch:'main'
        }
        }
        stage ('build') {
            steps {
                sh 'chmod +x commands.sh'
                sh './commands.sh'
        }
        }
    }
}